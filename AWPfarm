	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").UpgradeSPM.CFrame * CFrame.new(0,3,0)
end
local function UP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").RPMapExpansion.Ultra.CFrame * CFrame.new(0,3,0)
end
local function MP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").RPMapExpansion.Mega.CFrame * CFrame.new(0,3,0)
end
local function OP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").RPMapExpansion.Omega.CFrame * CFrame.new(0,3,0)
end
local function AP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Ascend.CFrame * CFrame.new(0,3,0)
end

game.Players.LocalPlayer.Chatted:Connect(function(msg)
	if msg == "AWPF" then
		if not farm then
			farm = true
			while farm do
				for count = 0, 5, 1 do
					Quality()
					wait(1)
					RP()
					wait(0.3)
					UP()
					wait(0.3)
					MP()
					wait(0.3)
					OP()
					wait(0.3)
				end
				for count1 = 0, 5, 1 do
					Quality()
					wait(1)
					RP()
					wait(0.3)
					UP()
					wait(0.3)
					MP()
					wait(0.3)
				end
				for count2 = 0, 5, 1 do
					Quality()
					wait(1)
					RP()
					wait(0.3)
					UP()
					wait(0.3)
				end
				for count3 = 0, 3, 1 do
					Quality()
					wait(1)
					RP()
					wait(0.3)
				end
				for count4 = 0, 2, 1 do
					Rarity()
					wait(1)
					TP()
					wait(0.3)
				end
				for count5 = 0, 5, 1 do
					if APMcheck == 0 or APMcheck > 0 and APMcheck < 3 then
						Rarity()
						wait(1)
						TP()
						wait(0.3)
						APMTP()
						wait(0.3)
						LuckTP()
						wait(0.3)
						PPMTP()
						wait(0.3)
						SPMTP()
						wait(0.3)


					elseif APMcheck == 3 then
						Rarity()
						wait(1)
						TP()
						wait(0.3)
						LuckTP()
						wait(0.3)
						PPMTP()
						wait(0.3)
						SPMTP()	
						wait(0.3)
					end
				end
				for count7 = 0, 4, 1 do
					Rarity()
					wait(1)
					AP()
					wait(0.3)
					APMAP()
					wait(0.3)
					LuckAP()
					wait(0.3)
					SPMAP()
					wait(0.3)
					PPMAP()
					wait(0.3)
				end
				for count8 = 0, 5, 1 do
					if PPMcheck == 0 or PPMcheck > 0 and PPMcheck < 3 then
						Rarity()
						wait(1)
						PP()
						wait(0.3)
						PPMPP()
						wait(1)
						LuckPP()
						wait(1)
						SPMPP()
						wait(0.5)
						PPMcheck= PPMcheck + 1
					elseif PPMcheck == 3 then
						Rarity()
						wait(1)
						PP()
						wait(0.3)
						LuckPP()
						wait(1)
						SPMPP()
						wait(0.5)
					end
				end
				for count9 = 0, 10, 1 do
					Rarity()
					wait(1)
					SP()
					wait(0.3)
					Luck()
					wait(2)
					RollCD()
					wait(0.5)
					Bulk()
					wait(0.5)
					SPM()
					wait(2)
				end
				Rarity()
				wait(1)
				AWP()
				wait(1)
				AWPUPG()
				wait(1)
                AWPcheck = AWPcheck + 1
                if AWPcheck == 15 then
                    for count9 = 0,30,1 do
                        AWPluck()
                        wait(0.1)
                        OPMAWP()
                        wait(0.1)
                        MPMAWP()
                        wait(0.1)
                        UPMAWP()
                        wait(0.1)
                        RPMAWP()
                        wait(0.1)
                        SPMAWP()
                        wait(0.1)
                        PPMAWP()
                        wait(0.1)
                        APMAWP()
                        wait(0.1)
                    end
                    AWPcheck=0
                end
				APMcheck = 0
				PPMcheck = 0
			end
		else
			farm = false
			endwaittime = 0
		end
	end
end)
