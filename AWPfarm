farm = false
local endwaittime = 0
local PPMcheck = 0
local APMcheck = 0
local AWPcheck = 0
local function AWP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").OPMapExpansion.Awakening.CFrame * CFrame.new(0,3,0)
end
local function AWPUPG()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").OPMapExpansion.UpgradeAWPMAWP.CFrame * CFrame.new(0,3,0)
end
local function RPMAWP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").OPMapExpansion.UpgradeRPMAWP.CFrame * CFrame.new(0,3,0)
end
local function UPMAWP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").OPMapExpansion.UpgradeUPMAWP.CFrame * CFrame.new(0,3,0)
end
local function MPMAWP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").OPMapExpansion.UpgradeMPMAWP.CFrame * CFrame.new(0,3,0)
