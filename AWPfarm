local function PPMAWP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").OPMapExpansion.UpgradePPMAWP.CFrame * CFrame.new(0,3,0) 
end
local function APMAWP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").OPMapExpansion.UpgradeAPMAWP.CFrame * CFrame.new(0,3,0)
end
local function AWPLuck()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").OPMapExpansion.UpgradeAWPLuck.CFrame * CFrame.new(0,3,0) 
end
local function Warp()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").OPMapExpansion.Warpstuff.Warp.CFrame * CFrame.new(0,3,0)
end
local function TP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").APMapExpansion2.Transcend.CFrame * CFrame.new(0,3,0)
end
local function PPMTP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").APMapExpansion2.UpgradePPMTP.CFrame * CFrame.new(0,3,0)
end
local function SPMTP() 
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").APMapExpansion2.UpgradeSPMTP.CFrame * CFrame.new(0,3,0)
end
local function APMTP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").APMapExpansion2.UpgradeAPMTP.CFrame * CFrame.new(0,3,0)
end
local function LuckTP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").APMapExpansion2.UpgradeLuckTP.CFrame * CFrame.new(0,3,0)
end
local function Quality()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").TPMapExpansion.QualityGet.CFrame * CFrame.new(0,3,0)
end
local function RP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").TPMapExpansion.Rebirth.CFrame * CFrame.new(0,3,0)
end
local function PPMAP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").APMapExpansion.UpgradePPMAP.CFrame * CFrame.new(0,3,0)
end
local function SPMAP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").APMapExpansion.UpgradeSPMAP.CFrame * CFrame.new(0,3,0)
end
local function APMAP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").APMapExpansion.UpgradeAPMAP.CFrame * CFrame.new(0,3,0)
end
local function LuckAP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").APMapExpansion.UpgradeLuckAP.CFrame * CFrame.new(0,3,0)
end
local function PP()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Prestige.CFrame * CFrame.new(0,3,0)
end
local function Rarity()
	game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").RarityGet.CFrame * CFrame.new(0,3,0)
end
